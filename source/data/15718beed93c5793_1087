#include <stdio.h>

int main(int argc, char **argv)
{
        int i=0, p;
		int p1,p2, p3,p4;

		p1 = fork(); p = wait();
		p2 = fork(); p = wait();
		p3 = fork(); p = wait();
		p4 = fork(); p = wait();

		printf("Cur process = %d     Parent = %d      Fork return Values =   ", getpid(), getppid());
		printf("%d %d %d %d \n", p1,p2,p3,p4);

		return 0;

}



